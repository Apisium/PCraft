<% imports.forEach(i => { %>import <%= i.name %> from '<%= i.path %>'
<% }) %>

export default interface <%= name %> <%= exts.length ? 'extends ' + exts.join(', ') + ' ' : '' %>{
  <% readonlys.forEach(i => { %>readonly <%= i.name %>: <%= i.type %>
  <% }) %>
  <% fields.forEach(i => { %><%= i.name %>: <%= i.type %>
  <% }) %>
  <% methods.forEach(i => { %><%= i.name %> (<%= i.args.map(a => a.name + ': ' + a.type).join(', ') %>): <%= i.type %>
  <% }) %>
}
